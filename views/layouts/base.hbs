<html data-theme="garden" lang="en">

<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <title>Foliaging Admin Panel</title>

  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link
    href="https://fonts.googleapis.com/css2?family=Lato:ital,wght@0,100;0,300;0,400;0,700;0,900;1,100;1,300;1,400;1,700;1,900&display=swap"
    rel="stylesheet">
  <link rel="stylesheet" href="/assets/css/tailwind.output.css" />
  {{#block "head"}}{{/block}}
</head>

<body class="bg-gradient-to-tl from-green-400 to-amber-900 dark:from-stone-500 dark:to-neutral-900">
  {{> header }}

  <div class="mx-auto h-full bg-no-repeat bg-fixed bg-cover bg-plant-pattern">
    <div class="w-full h-auto">
      {{> alert}}
      {{> status}}
      {{#block "content"}}{{/block}}
    </div>
  </div>

  <script src="/assets/js/main.js"></script>
  {{#block "js"}}{{/block}}
</body>

</html>